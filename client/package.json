    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prisma": "^4.7.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "**/*.(ts,tsx)": [
      "yarn pre-commit-lint",
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
