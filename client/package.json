    "@react-three/drei": "^9.45.0",
    "@react-three/fiber": "^8.9.1",
    "@segment/analytics-next": "^1.46.4",
    "@tanstack/react-query": "^4.18.0",
    "@thirdweb-dev/react": "^3.6.6",
    "@thirdweb-dev/sdk": "^3.6.6",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "cuid": "^2.1.8",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.4",
    "ethers": "^5.7.2",
    "gsap": "^3.11.3",
    "husky": "^8.0.2",
    "leva": "^0.9.34",
    "lint-staged": "^13.0.4",
    "next": "13.0.4",
    "next-auth": "^4.17.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.40.0",
    "sass": "^1.56.1",
    "sharp": "^0.31.2",
    "siwe": "^2.1.3",
    "three": "^0.146.0",
    "typescript": "4.9.3",
    "wagmi": "^0.8.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.11",
